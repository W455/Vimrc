#include <unistd.h>

int	main(int ac, char **av)
{
	int i;
	int j;
	int k = 0;
	int e = 10;

	i = 0;
	j = 0;
	if (ac == 3)
	{
		while (av[1][i])
		{
			while (av[1][k] != av[1][i] && av[1][k])
				k++;
			if (k == i)
				write(1, &av[1][i], 1);
			k = 0;
			i++;
		}
		i = 0;
		k = 0;
		while (av[2][j])
		{
			while (av[2][k] != av[2][j] && av[2][k])
				k++;
			while (av[2][j] != av[1][i] && av[1][i])
				i++;
			if (!av[1][i] && k == i)
				write(1, &av[2][j], 1);
			j++;
			k = 0;
			i = 0;
		}
	}
	write(1, &e, 1);
}
