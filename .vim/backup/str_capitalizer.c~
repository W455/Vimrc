#include <unistd.h>
#include <stdio.h>

int		main(int ac, char **av)
{
char str1[5];
str1[0] = 66;
str1[1] = 105;
str1[2] = 116;
str1[3] = 99; 
str1[4] = 104;
printf("%s", str1);
}
